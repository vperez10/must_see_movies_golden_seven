<h1> Movie Listing </h1>

<table>
  <tr>
    <th>Movie Image</th>
    <th>Movie Title</th>
    <th>Year</th>
    <th>Duration</th>
    <th colspan="3">Description</th>
  </tr>
  <% @list_of_movies do |title| %>
  <tr>
    <td>
      <img src="<%= movies.image_url %>" width="80">
    </td>
    <td>
      <%= movies.title %>
    </td>
  </tr>
  <% end %>
</table>


